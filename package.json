{
  "name": "@bitskyai/ui",
  "version": "0.4.1",
  "private": true,
  "description": "UI for BitSky",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build-electron": "cross-env UMI_ENV=electron umi build",
    "build-admin": "cross-env UMI_ENV=admin umi build",
    "build-landing": "cross-env UMI_ENV=landing umi build",
    "build": "cross-env UMI_ENV=ui umi build",
    "format-imports": "import-sort --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "cross-env UMI_ENV=ui umi dev",
    "start-electron": "cross-env UMI_ENV=electron umi dev",
    "start-admin": "cross-env UMI_ENV=admin umi dev",
    "start-landing": "cross-env UMI_ENV=landing umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^4.5.9",
    "@antv/data-set": "^0.10.2",
    "antd": "^3.20.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.16",
    "enquire-js": "^0.2.1",
    "highlight.js": "^10.3.2",
    "immer": "^3.2.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.20",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.4",
    "moment": "^2.29.1",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "rc-queue-anim": "^1.7.0",
    "rc-scroll-anim": "^2.5.9",
    "rc-tween-one": "^2.5.0",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-github-button": "^0.1.11",
    "react-media": "^1.9.2",
    "react-media-hook2": "^1.0.5",
    "react-responsive": "^8.0.1",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.1",
    "react-redux": "7.2.1",
    "umi": "^3.2.14",
    "umi-request": "^1.0.8",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "uuid": "8.1.0"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.133",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.9",
    "cross-env": "^5.2.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "slash2": "2.0.0",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "1.6.19",
    "@umijs/fabric": "^2.3.1",
    "@umijs/yorkie": "^2.0.3",
    "webpack-theme-color-replacer": "^1.3.14"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
